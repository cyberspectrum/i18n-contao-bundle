<?xml version="1.0"?>
<psalm
    hideExternalErrors="true"
    errorLevel="1"
    resolveFromConfigFile="true"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://getpsalm.org/schema/config"
    xsi:schemaLocation="https://getpsalm.org/schema/config vendor/vimeo/psalm/config.xsd"
>
    <projectFiles>
        <directory name="src" />
    </projectFiles>
    <issueHandlers>
        <UnusedClass>
            <errorLevel type="suppress">
                <!-- Loaded via symfony bundle -->
                <referencedClass name="CyberSpectrum\I18N\ContaoBundle\DependencyInjection\CyberSpectrumI18NContaoExtension"/>
            </errorLevel>
        </UnusedClass>
    </issueHandlers>
</psalm>
